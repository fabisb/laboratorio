{
  "name": "medlab-front",
  "version": "0.1.3",
  "description": "MedLab-Suite, La Milagrosa",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder --ia32 --x64",
    "obf": "javascript-obfuscator ./app/javascript --output ../encriptadoFront/app/javascript"
  },
  "postinstall": "bower install",
  "build": {
    "appId": "com.coditados.MedLab",
    "productName": "MedLab",
    "win": {
      "target": "nsis",
      "icon":"build/icon.ico"
    },
    "directories": {
      "output": "dist"
    }
  },
  "repository": "https://github.com/fabisb/MedLab",
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "author": "Fabian",
  "devDependencies": {
    "electron": "^26.6.5",
    "electron-builder": "^24.13.3",
    "electron-reload": "^2.0.0-alpha.1",
    "javascript-obfuscator": "^4.1.1"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "bigeval": "^3.2.2",
    "bootstrap": "^5.3.3",
    "bower": "^1.8.14",
    "chart.js": "^4.4.3",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.2.1",
    "moment": "^2.30.1"
  }
}
